{
  "name": "storytime_be",
  "version": "0.0.1",
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "description": "Storytime API",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:pm2:dev": "npx pm2 startOrReload ecosystem.config.js --only storytime-api-development --update-env",
    "start:pm2:staging": "npx pm2 startOrReload ecosystem.config.js --only storytime-api-staging --update-env",
    "start:pm2:prod": "npx pm2 startOrReload ecosystem.config.js --only storytime-api-production --update-env",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate deploy",
    "make-migration": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "npx ts-node prisma/seeds/index.ts",
    "deploy:dev": "pnpm make-migration && pnpm generate && pnpm start:pm2:dev",
    "deploy:staging": "pnpm make-migration && pnpm generate && pnpm start:pm2:staging",
    "deploy:prod": "pnpm make-migration && pnpm generate && pnpm start:pm2:prod"
  },
